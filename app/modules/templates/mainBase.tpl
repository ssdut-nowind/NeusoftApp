<div class="main-page-border" data-module="{module}">
    <div class="main-page-header">
        <div class="main-header-left" data-bind="click: showSlideMenu">
            <div class="main-header-icon">
                <i class="fa fa-bars"></i>
            </div>
        </div>
        <div class="main-header-title" data-bind="text: title"></div>
        <div class="main-header-right" data-bind="click: showCenter">
            <div class="main-header-icon">
                <i class="fa fa-user"></i>
            </div>
        </div>
    </div>
    <div class="main-page-body" data-scrollable="true">
    </div>
</div>